# Print Server Configuration
# Copy this file to .env and customize as needed

# Server Settings
PRINT_SERVER_HOST=0.0.0.0
PRINT_SERVER_PORT=5000
DEBUG=False

# Mock Mode - Set to True for testing without physical printers
MOCK_MODE=True

# Printer Names
# These should match the actual printer names in your system
# For mock mode, these can be any names
PRINTER_A_NAME=PrinterA
PRINTER_B_NAME=PrinterB

# Logging
LOG_LEVEL=INFO

# Print Job Settings
MAX_RETRIES=3
RETRY_DELAY=2
